{
  "manifest_version": 3,
  "name": "Table Toggle Extension",
  "version": "1.0",
  "description": "Toggles line clamp of commit messages in Mendix Teamserver",
  "permissions": ["scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "run_script": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      },
      "description": "Toggles commit messages"
    }
  }
}